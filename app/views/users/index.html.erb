<h1>Listing Users</h1>
<table>
<tr>
	<td>First</td>
	<td>Last</td>
	<td>Email</td>
	<td>Actions</td>
</tr>
<% @users.each do |user| %>
<tr>
	<td><%= user.id %></td>
	<td><%= user.first_name %></td>
	<td><%= user.last_name %></td>
	<td><%= user.email %></td>
	<td><a href="/users/<%= user.id %>">Show</a></td>
	<td><a href="/users/<%= user.id %>/edit">Edit</a></td>
	<td><a href="/users/<%= user.id %>" data-method='delete'>Destroy</a></td>
</tr>
<% end %>
</table>

<h2><a href="/users/new">New User</a></h2>


